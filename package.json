{
  "name": "vite-electron-react-tpl-v2",
  "version": "1.0.0",
  "author": "app",
  "appName": "Vite Demo",
  "appId": "hefx.vite_electron_vue_tpl_v2.com",
  "description": "demo",
  "private": true,
  "engines": {
    "node": ">=v14.16.0",
    "npm": ">=7.6.3"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "buildEnvTypes": "node scripts/buildEnvTypes.js",
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile-win": "npm run precompile && electron-builder build --config electron-builder.config.js --win",
    "compile-mac": "npm run precompile && electron-builder build --config electron-builder.config.js --mac",
    "pretest": "cross-env MODE=test npm run build",
    "test": "node tests/app.spec.js",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "pretypecheck": "npm run buildEnvTypes",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer",
    "unpack": "npx asar extract dist/win-unpacked/resources/app.asar dist/asar-unpacked/ ",
    "build:less": "lessc packages/renderer/src/styles/base.less packages/renderer/src/styles/base.css"
  },
  "browserslist": [
    "Chrome 89"
  ],
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/md5": "^2.3.0",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/request-promise": "^4.1.47",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "autoprefixer": "^9.8.8",
    "chokidar": "^3.5.1",
    "cross-env": "^7.0.3",
    "electron": "^12.0.7",
    "electron-builder": "^22.11.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.22.0",
    "less": "^4.1.1",
    "md5": "^2.3.0",
    "postcss": "^7.0.39",
    "prettier": "^2.2.1",
    "spectron": "^14.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.4",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2",
    "vite": "^2.3.1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "ahooks": "^3.0.2",
    "antd": "^4.15.5",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.8",
    "getmac": "^5.17.0",
    "qs": "^6.10.0",
    "react": "^17.0.2",
    "react-custom-scrollbars-2": "^4.4.0",
    "react-dom": "^17.0.2",
    "react-quill": "^2.0.0-beta.4",
    "react-router-dom": "^5.2.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  }
}
